		"safari 5",
		"ios 6",
		"android 4"
	],
	"config": {
		"//": "Entry files",
		"host": "127.0.0.1",
		"port": 3000,
		"baseUrl": "./",
		"directories": {
			"source": "src",
			"destination": "docs",
			"temporary": "tmp",
			"//": "Directories relative to `source` directory",
			"modules": "_modules",
			"layouts": "_layouts",
			"images": "_images",
