{
	"compilerOptions": {
		"strict": true,
		"alwaysStrict": true,
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": true,
		"target": "es5",
		"lib": ["es2015", "es2017", "dom"],
		"baseUrl": "../node_modules",
		"moduleResolution": "node",
		"pretty": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"removeComments": true,
		"noImplicitThis": false,
		"allowJs": true
	}
}
